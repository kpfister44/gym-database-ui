    "dev": "concurrently \"npm run watch:backend\" \"npm run watch:frontend\" \"npm run start:server\"",
    "start": "ts-node src/backend/server.ts",
    "serve": "concurrently \"npm run build:backend\" \"npm run build:frontend\" \"npm run start:server\""
